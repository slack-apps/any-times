{
  "name": "@slack-apps/any-times",
  "version": "0.0.0",
  "description": "Collect messages in times-*",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage .esbuild",
    "deploy": "serverless deploy",
    "lint": "eslint --format=pretty --cache --max-warnings 0 --fix --ext .ts .",
    "local": "serverless offline --noPrependStageInUrl",
    "ngrok": "ngrok http 3000",
    "prebuild": "npm run lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@slack/bolt": "3.12.2",
    "@slack/web-api": "6.8.1",
    "runtypes": "6.5.1",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@serverless/typescript": "3.27.0",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "esbuild": "0.17.11",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-formatter-pretty": "6.0.1",
    "eslint-plugin-jest": "28.8.3",
    "jest": "27.5.1",
    "prettier": "3.3.3",
    "rimraf": "3.0.2",
    "serverless": "3.28.1",
    "serverless-esbuild": "1.39.0",
    "serverless-offline": "8.8.1",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "14.21.3"
  },
  "suggest": {
    "@commitlint/{cli,config-conventional}": "Checks if your commit messages meet the conventional commit format",
    "concurrently": "Run multiple commands concurrently",
    "fast-json-stringify": "faster than JSON.stringify()",
    "iterare": "Applying multiple transformations to a collection in a single iteration",
    "pino": "Very low overhead Node.js logger"
  },
  "volta": {
    "node": "14.21.3"
  }
}
